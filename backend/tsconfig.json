{
  "compilerOptions": {
    "lib": ["esnext", "dom"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ESNext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": "./src",
    "rootDir": "./",
    "outDir": "./dist",
    "paths": {
      "@/*": ["./src/*"],
      "@/commons/*": ["./commons/*"],
      "@/middlewares/*": ["./middlewares/*"],
      "@/mailers/*": ["./mailers/*"],
      "@/env": ["./config/env/env.ts"],
      "@/services/*": ["./services/*"],
      "@/utils": ["./utils/*"],
      "@/internal": ["./internal/*"],
      "@/modules/*": ["./modules/*"],
      "@/lib/*": ["./lib/*"],
      "@/db/*": ["./config/db/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  }
}
